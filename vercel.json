{
  "version": 2,
  "rewrites": [
    { "source": "/assets/models/buildingdetail/:file", "destination": "/assets/models/buildingDetail/:file" },
    { "source": "/assets/models/buildingDetail/:file", "destination": "/assets/models/buildingDetail/:file" },
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|json|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "buildCommand": "node src/helpers/ensureDirectories.js && npm run build"
}
